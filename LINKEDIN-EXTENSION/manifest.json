{
  "manifest_version": 3,
  "name": "LinkedIn Auto Interaction",
  "version": "1.1",
  "description": "Auto like and comment on LinkedIn feed.",
  "permissions": ["scripting", "tabs", "activeTab"],
  "host_permissions": ["https://*.linkedin.com/*"],
  "action": {
    "default_popup": "popup.html"
  }
}